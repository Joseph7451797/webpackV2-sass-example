{
  "name": "webpackv2-sass-example",
  "version": "1.0.0",
  "description": "webpack V2 sass/scss setting example, with html-webpack-plugin, postcss-loader",
  "main": "index.js",
  "scripts": {
    "hot-dev": "./node_modules/.bin/webpack-dev-server --hot --progress --inline",
    "host": "NODE_ENV=host ./node_modules/.bin/webpack-dev-server --no-info --hot --inline --host $(ifconfig | grep -Eo 'inet (addr:)?([0-9]*\\.){3}[0-9]*' | grep -Eo '([0-9]*\\.){3}[0-9]*' | grep -v '127.0.0.1')",
    "build": "NODE_ENV=build ./node_modules/.bin/webpack --progress --profile --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joseph7451797/webpackV2-sass-example.git"
  },
  "keywords": [
    "webpack",
    "sass"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joseph7451797/webpackV2-sass-example/issues"
  },
  "homepage": "https://github.com/Joseph7451797/webpackV2-sass-example#readme",
  "devDependencies": {
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-css-reset": "^1.0.2",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  }
}
